{
  "compilerOptions": {
    "outDir": "./built",
    "allowJs": false,
    "target": "es2016",
    "module": "esnext",
    "moduleResolution": "node",
    "sourceMap": false,
    "strict": false,
    "esModuleInterop": true,
    "newLine": "LF",
    "removeComments": false,
    "resolveJsonModule": true
  },
  "include": ["./src/**/*"],
  "exclude": [
    "node_modules",
    "TODO.txt"
  ]
}